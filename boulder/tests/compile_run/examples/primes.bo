fn is_prime(n: u32) -> Bool {
    if n < 2 {
        False
    } else {
        is_prime_recurse(n, n - 1)
    }
}

fn is_prime_recurse(n: u32, i: u32) -> Bool {
    if i < 2 {
        True
    } else {
        if n % i < 1 {
            False
        } else {
            is_prime_recurse(n, i - 1)
        }: Bool
    }
}

@test
fn five() -> Bool {
    is_prime(5)
}

@test
fn one_hundred() -> Bool {
    !is_prime(100)
}