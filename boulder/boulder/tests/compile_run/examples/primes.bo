# fn is_prime(5) -> True
# fn is_prime(1000) -> False
# fn mod(5,3) -> 2

fn is_prime(n: u32) -> Bool {
    if n < 2 {
        False
    } else {
        is_prime_recurse(n, n - 1)
    }
}

fn is_prime_recurse(n: u32, i: u32) -> Bool {
    if i < 2 {
        True
    } else {
        if mod(n,i) < 1{
            False
        } else {
            is_prime_recurse(n, i - 1)
        }: Bool
    }
}

fn mod(n: u32, m: u32) -> u32 {
    n - (n/m * m)
}
