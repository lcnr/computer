fn is_prime(n: u32) -> Bool {
    match n < 2 {
        _: True -> False,
        _: False -> is_prime_recurse(n, n - 1)
    }
}

fn is_prime_recurse(n: u32, i: u32) -> Bool {
    match i < 2 {
        _: True -> True,
        _: False -> match mod(n,i) < 1{
            _: True -> False,
            _: False -> is_prime_recurse(n, i - 1),
        }: Bool
    }
}

fn mod(n: u32, m: u32) -> u32 {
    n - (n/m * m)
}
