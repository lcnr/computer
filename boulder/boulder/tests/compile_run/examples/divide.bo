# fn divide(1,1) -> 1
# fn divide(5,10) -> 0
# fn divide(10,5) -> 2

fn divide(a: u32, b: u32) -> u32 {
    let i = 0;
    let new_b: u32 = b;
    while new_b << 1 > new_b {
        i = i + 1;
        new_b = new_b << 1;
    };
    i = i + 1;

    let result = 0;
    while i > 0 {
        i = i - 1;
        if (b << i) - 1 < a {
            a = a -  (b << i);
            result = result | (1 << i);
        }: Empty;
    };

    result
}
