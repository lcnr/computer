# fn invert(True) -> False
# fn iff() -> 9
# fn iff_empty() -> 7

fn invert(b: Bool) -> Bool {
    if b {
        False
    } else {
        True
    }: Bool
}

fn iff() -> u32 {
    if invert(True) {
        3
    } else {
        4
    } + if True {
        5
    } else {
        6
    }
}

fn iff_empty() -> u32 {
    let x: u32 | Empty = if True {
        7: u32
    };

    match x {
        n: u32 -> n,
        _: Empty -> 8,
    }
}
