# the stack pointer MEM[1][0] should always point above the last element on the stack
store:
    # store B, C and D on the stack
    setb 1;
    sets 0;
    load MEM A;
    sets A;
    store B;
    load 1 B;
    add A;
    sets A;
    store C;
    add A;
    sets A;
    store D;
    add A; # update stack pointer
    sets 0;
    store A;
load:
    # load D and C from the stack
    setb 1;
    sets 0;
    load MEM A;
    load 1 B;
    sub A;
    sets A;
    load MEM D;
    sub A;
    sets A;
    load MEM C;
    sets 0; # update stack pointer
    store A;

